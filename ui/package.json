{
  "name": "articulate",
  "version": "0.0.0",
  "description": "Smart conversational interfaces",
  "repository": {
    "type": "git",
    "url": "git://github.com/samtecspg/articulate.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=10.5.0"
  },
  "author": "Samtec Smart Platform Group",
  "license": "Apache-2.0",
  "main": "server/index.js",
  "scripts": {
    "analyze": "node ./internals/scripts/analyze.js",
    "start": "cross-env NODE_ENV=development env-cmd .env nodemon server",
    "start:prod": "cross-env NODE_ENV=production env-cmd .env node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true nodemon server",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "preinstall": "npm run npmcheckversion",
    "postinstall": "npm run build:dll",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint './app/**/*.js'",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts .",
    "lint:eslint:fix": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --fix .",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "prettify": "prettier --write"
  },
  "keywords": [],
  "dependencies": {
    "@material-ui/core": "3.6.1",
    "@material-ui/icons": "3.0.1",
    "@material-ui/lab": "3.0.0-alpha.24",
    "history": "4.7.2",
    "intl": "1.2.5",
    "invariant": "2.2.4",
    "javascript-time-ago": "2.0.1",
    "lodash": "4.17.11",
    "material-colors": "1.2.6",
    "minimist": "1.2.0",
    "nes": "6.5.2",
    "prop-types": "15.7.2",
    "query-string": "6.2.0",
    "react": "16.8.3",
    "react-ace": "6.1.4",
    "react-autosuggest": "9.4.2",
    "react-color": "2.14.1",
    "react-dnd": "7.0.2",
    "react-dnd-html5-backend": "7.0.2",
    "react-dom": "16.8.3",
    "react-intl": "2.4.0",
    "react-loadable": "5.4.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "4.0.0",
    "redux-saga": "0.16.0",
    "redux-seamless-immutable": "0.4.0",
    "reselect": "3.0.1",
    "seamless-immutable": "7.1.3",
    "shouldcomponentupdate-children": "^1.1.0",
    "swagger-client": "3.4.4",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "3.1.3",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-react-intl": "2.4.0",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chalk": "2.4.1",
    "circular-dependency-plugin": "5.0.2",
    "cross-env": "5.2.0",
    "css-loader": "2.1.0",
    "env-cmd": "8.0.2",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-webpack": "0.10.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.9.1",
    "eslint-plugin-redux-saga": "0.9.0",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.7.0",
    "file-loader": "1.1.11",
    "glob": "7.1.3",
    "hapi": "18.1.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "4.3.1",
    "imports-loader": "0.8.0",
    "ip": "1.1.5",
    "ngrok": "^3.1.1",
    "node-plop": "0.15.0",
    "nodemon": "1.18.10",
    "offline-plugin": "5.0.6",
    "prettier": "^1.16.4",
    "react-scripts": "2.1.5",
    "rimraf": "2.6.2",
    "shelljs": "0.8.3",
    "style-loader": "0.21.0",
    "svg-url-loader": "2.3.2",
    "url-loader": "1.0.1",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8",
    "webpack-dashboard": "3.0.0",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.2",
    "webpack-pwa-manifest": "3.6.2"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "node_modules"
    ],
    "delay": "2500"
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "env-cmd"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "babel": {
    "plugins": [],
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-inline-elements",
          "transform-react-constant-elements"
        ]
      },
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs",
          "dynamic-import-node"
        ]
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged"
}
